{
    "name": "drupal/powerstack_page_builder",
    "type": "drupal-module",
    "description": "Basic setup for the Drupal Page Builder settings",
    "version": "0.0.1",
    "keywords": [
        "Drupal"
    ],
    "license": "GPL-2.0+",
    "repositories": {
        "composer-2-workspaces": {
            "type": "vcs",
            "url": "git@github.com:xaviemirmon/composer-2-workspaces-plugin.git"
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "drupal/paragraphs": "^1.14",
        "drupal/field_group": "^3.2",
        "drupal/pathauto": "^1.8"
    },
    "require-dev": {
        "tmdk/composer-workspaces-plugin": "dev-overrides"
    },
    "extra": {
        "workspace-root": "../.."
    },
    "config": {
        "allow-plugins": {
            "tmdk/composer-workspaces-plugin": true
        }
    }
}
